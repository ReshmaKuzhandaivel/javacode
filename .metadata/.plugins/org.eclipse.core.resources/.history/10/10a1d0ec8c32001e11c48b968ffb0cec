package day4;

public class Loop {
	public static void main(String[]args) {
		int day=1;
		int no_of_classes=10;
		while(day<=no_of_classes) {
			System.out.println("I am attending class day"+day);
					day++;
		}
		int attemp=1;
		int max_attempt=0;
		do {
			System.out.println("This is attempt no"+attempt);
			attempt++;
		}
		
	}

}
